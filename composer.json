{
    "name": "martin-lechene/ebox-laravel",
    "description": "Laravel package for e-Box Enterprise integration with Belgian government messaging system",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Lechene",
            "email": "martin@lechene.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "guzzlehttp/guzzle": "^7.5",
        "ramsey/uuid": "^4.7",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Ebox\\Enterprise\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ebox\\Enterprise\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ebox\\Enterprise\\Providers\\EboxServiceProvider"
            ],
            "aliases": {
                "Ebox": "Ebox\\Enterprise\\Facades\\Ebox"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

